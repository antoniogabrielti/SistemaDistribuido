package Atividade1;

import java.util.logging.Level;
import java.util.logging.Logger;

public class Thread1 implements Runnable {

	@Override
	public void run() {
		
		
        try {
        	Thread1.sleep(200);
        } catch (InterruptedException ex) {
            Logger.getLogger(Thread1.class.getName()).log(Level.SEVERE, null, ex);
        }
		
	}

}
